{{>nav}}
  {{#if delSelfError}}
    <div style="text-align: center;" class="alert alert-danger" role="alert">
      {{delSelfError}}
    </div>
  {{/if}}
<h1 class="d-flex justify-content-center mt-2">Resources</h1>
<div class="mx-auto mt-4" style="width: 35%;">
  <table class="table bg-dark text-white" style="border-radius: 1em 1em 0 0">
    <thead>
      <tr>
        <th scope="col">Actions</th>
        <th scope="col">First Name</th>
        <th scope="col">Last Name</th>
        <th scope="col">email</th>
        <th scope="col">Cell Phone Number</th>
        <th scope="col">Actions</th>
        <th scope="col">TOTP</th>
      </tr>
    </thead>
    <tbody>
      {{#each dbo_user_accounts}}
      <tr>
        <td><a href="/changePassword{{this.resource_id}}">Change Password</a></td>
        <td>{{this.firstName}}</td>
        <td>{{this.lastName}}</td>
        <td>{{this.email}}</td>
        <td>{{this.cellNum}}</td>
        <td><a href="/deleteResource{{this.resource_id}}">Delete</a></td>
        <td><a href="/delete/totp/key{{this.resource_id}}">Reset TOTP</a></td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>
{{>toast}}